          "animation": {
            "duration": 500,
            "enabled": true,
            "type": "slide"
          },
          "blur_effect": {
            "acrylic": false,
            "border_color": "System",
            "dark_mode": false,
            "enabled": false,
            "round_corners": false,
            "round_corners_type": "normal"
