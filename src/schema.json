          "class_name": "yasb-bar",
          "context_menu": true,
          "dimensions": {
            "height": 30,
            "width": "100%"
          },
          "enabled": true,
          "layouts": {
            "center": {
              "alignment": "center",
              "stretch": true
            },
            "left": {
              "alignment": "left",
              "stretch": true
            },
            "right": {
              "alignment": "right",
              "stretch": true
            }
          },
          "padding": {
            "bottom": 0,
            "left": 0,
            "right": 0,
            "top": 0
