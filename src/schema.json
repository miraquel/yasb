  "additionalProperties": false,
  "properties": {
    "watch_config": {
      "default": true,
      "title": "Watch Config",
      "type": "boolean"
    },
    "watch_stylesheet": {
      "default": true,
      "title": "Watch Stylesheet",
      "type": "boolean"
    },
    "debug": {
      "default": false,
      "title": "Debug",
      "type": "boolean"
    },
    "env_file": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Env File"
    },
    "update_check": {
      "default": true,
      "title": "Update Check",
      "type": "boolean"
    },
    "show_systray": {
      "default": true,
      "title": "Show Systray",
      "type": "boolean"
    },
    "komorebi": {
      "$ref": "#/$defs/KomorebiConfig",
      "default": {
        "start_command": "komorebic start --whkd",
        "stop_command": "komorebic stop --whkd",
        "reload_command": "komorebic reload-configuration"
      }
    },
    "glazewm": {
      "$ref": "#/$defs/GlazeWMConfig",
      "default": {
        "start_command": "glazewm.exe start",
        "stop_command": "glazewm.exe command wm-exit",
        "reload_command": "glazewm.exe command wm-exit && glazewm.exe start"
