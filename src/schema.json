          "widgets": {
            "center": [],
            "left": [],
            "right": []
          },
          "window_flags": {
            "always_on_top": false,
            "auto_hide": false,
            "hide_on_fullscreen": false,
            "hide_on_maximized": false,
            "windows_app_bar": false
          }
        }
